@{
    Layout = "~/Views/Shared/Manage/RLayout.cshtml";
}
@section IncludeCSS{
    <link rel="stylesheet" href="~/Content/css/bootstrap-datetimepicker-build.css" />
}
@section IncludeScript{
    @Html.Partial("Manage/JSGlobalVar")
    <script>
        var gb_main_id = '@ViewBag.main_id';
        var gb_roles = '@ViewBag.RoleName';
    </script>
    <script src="~/Scripts/build/comminc.js"></script>
    <script src="~/Scripts/app/datetimepicker/bootstrap-datetimepicker.min.js"></script>
    <script src="~/Scripts/app/Sortable.min.js"></script>
    <script src="~/Scripts/app/react-sortable-mixin.js"></script>
    <script src="~/Scripts/build/func/m-AccountsPayable.js"></script>
    <script>
        var getRootDOM = document.getElementById('page_content');
        var rootParm = { MenuName: '@ViewBag.MenuName', Caption: '@ViewBag.Caption', IconClass: 'fa-usd' };
        var compObject = React.render(React.createElement(GirdForm, rootParm), getRootDOM);
    </script>
}
@section sidebar{
    @Html.Partial("Manage/vucMenu")
}

<ul class="breadcrumb">
    <li><a href="~/Active/AccountsPayable"><i class="fa-usd"></i> @ViewBag.MenuName</a></li>
    <li>@ViewBag.Caption</li>
</ul>